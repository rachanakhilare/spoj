#include <iostream>
#include<math.h>
using namespace std;

int main()
{
    int n,t;
    cin>>t;
    while(t--)
    {
    cin>>n;
    int num=pow(4,n-1)-1;

    if(n==2)
    {
        cout<<"YES"<<endl;
    }
    else
    {
    if(num%n==0)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }
    }
    }
    return 0;
}
